def fizz_buzz(x):
    for i in range(1, x + 1):
        if i % 3 == 0 and i % 5 == 0:
            print("FizzBuzz")
        elif i % 3 == 0:
            print("Fizz")
        elif i % 5 == 0:
            print("Buzz")
        else:
            print(i)

# Testando com os exemplos fornecidos
fizz_buzz(8)
print()  # Adicionando uma linha em branco entre os exemplos
fizz_buzz(14)
